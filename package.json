{
  "name": "rayect-starter",
  "version": "1.0.0",
  "description": "A simple starter package for Redux with React and Babel",
  "main": "webpack.config.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "postinstall": "webpack -p",
    "start": "node server.js",
    "dev": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "serve_dev": "webpack-dev-server",
    "serve_prod_win": "env NODE_ENV=production node server.js",
    "serve_prod": "env NODE_ENV=production node server.js",
    "build_win": "env NODE_ENV=production npm run clean && webpack -p",
    "build": "NODE_ENV=production npm run clean && webpack -p",
    "clean": "rimraf build",
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js --recursive ./test",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "React-Redux-Babel"
  ],
  "author": "Ray",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^4.1.1",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "image-webpack-loader": "^3.3.1",
    "jsdom": "^11.1.0",
    "mocha": "^3.5.0",
    "react-addons-test-utils": "^15.6.0",
    "rimraf": "^2.6.1",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.3",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-dev-server": "^2.7.1"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "express": "^4.15.4",
    "lodash": "^4.17.4",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "react-router": "^2.7.0",
    "react-router-dom": "^4.1.2",
    "redux": "^3.7.2",
    "redux-form": "^6.8.0",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.2.0",
    "webpack": "^3.5.3",
    "webpack-dev-middleware": "^1.12.0",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "image-webpack-loader": "^3.3.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-register": "^6.24.1"
  }
}
